{% flash_messages %}
 {% type.messages in app.session.flashbag.all() %}
  {% for message in messages %}
     <div class="alert alert-{{}} alert-dismissible" role="alert">
     	<button type="button" class="close" data-dismissable="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
     	{{ message | raw }}
     </div>
  {% endfor %}
 {% endfor %}
{% endblock %}     